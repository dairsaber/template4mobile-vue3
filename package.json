{
  "name": "vite-vue-demo",
  "version": "0.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "vite --host",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "test:unit": "jest",
    "lint:eslint": "eslint \"{src,mock}/**/*.{vue,ts,tsx,js}\" --fix",
    "lint:prettier": "prettier --write  \"src/**/*.{js,json,tsx,css,less,scss,vue,html,md}\""
  },
  "dependencies": {
    "axios": "^0.21.3",
    "js-cookie": "^3.0.1",
    "lodash-es": "^4.17.21",
    "moment": "^2.29.1",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.0-rc.7",
    "qs": "^6.10.1",
    "vant": "^3.2.2",
    "vue": "^3.2.9",
    "vue-router": "4.0.11"
  },
  "devDependencies": {
    "@types/jest": "^27.0.1",
    "@types/js-cookie": "^2.2.7",
    "@types/lodash-es": "^4.17.4",
    "@types/node": "^16.7.10",
    "@types/nprogress": "^0.2.0",
    "@types/qs": "^6.9.7",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "@vitejs/plugin-legacy": "^1.5.2",
    "@vitejs/plugin-vue": "^1.6.0",
    "@vitejs/plugin-vue-jsx": "^1.1.7",
    "@vue/cli-plugin-babel": "^4.5.13",
    "@vue/cli-plugin-unit-jest": "^4.5.13",
    "@vue/compiler-sfc": "^3.2.9",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "@vue/test-utils": "^2.0.0-rc.14",
    "autoprefixer": "^10.3.4",
    "chalk": "^4.1.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-define-config": "^1.0.9",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^7.17.0",
    "lint-staged": "^11.1.2",
    "mockjs": "^1.1.0",
    "postcss": "^8.3.6",
    "prettier": "^2.3.2",
    "sass": "^1.39.0",
    "sass-loader": "^12.1.0",
    "scss": "^0.2.4",
    "tailwindcss": "^2.2.9",
    "typescript": "4.4.2",
    "vite": "^2.5.3",
    "vite-plugin-components": "^0.13.3",
    "vite-plugin-compression": "^0.3.5",
    "vite-plugin-mock": "^2.9.6",
    "vite-plugin-style-import": "^1.2.1",
    "vite-plugin-svg-icons": "^1.0.4",
    "vite-plugin-vue-setup-extend": "^0.1.0",
    "vue-jest": "^5.0.0-alpha.10",
    "vue-tsc": "^0.3.0",
    "yorkie": "^2.0.0"
  },
  "gitHooks": {
    "commit-msg": "node ./build/scripts/verifyCommits.js",
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": "eslint --fix",
    "*.{js,json,tsx,css,less,scss,vue,html,md}": "prettier"
  }
}
